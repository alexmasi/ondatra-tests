{
    "git.autoRepositoryDetection": false,
    "go.testFlags": [
        // "-v",
        // "-timeout",
        // "60s",
        // "-config",
        // "../resources/global/knebind-config.yaml",
        // "-testbed",
        // "../resources/testbed/ixia-arista-ixia.txt"
    ],
    "editor.rulers": [
        80,
        90
    ],
    "go.useLanguageServer": true,
    "go.testEnvVars": {
        "CGO_ENABLED": 0,
    },
    "editor.formatOnSave": true,
    "gopls": {
        "build.env": {
            "CGO_ENABLED": 0,
        }
    },
    "go.toolsEnvVars": {
        "CGO_ENABLED": 0
    },
    "go.delveConfig": {
        "debugAdapter": "legacy"
    },
    "files.insertFinalNewline": true,
    "files.eol": "\n",
    "diffEditor.ignoreTrimWhitespace": false,
}
